<Window x:Class="AdditionalTools.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdditionalTools"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Tile Exporter">
                <Grid Background="#FFE5E5E5">
                    <RichTextBox Name="TilesRichTextBox" Margin="0,0,584,0" Width="NaN">
                        <FlowDocument>
                        </FlowDocument>
                    </RichTextBox>
                    <Button Content="Download" HorizontalAlignment="Left" Margin="215,33,0,0" VerticalAlignment="Top" Click="Button_Click"/>
                    <TextBox Name="pathTextBox" HorizontalAlignment="Left" Margin="215,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="533"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
